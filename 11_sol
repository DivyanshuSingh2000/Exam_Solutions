-- Create the Student table
CREATE TABLE Student (
    ID INT PRIMARY KEY NOT NULL,
    NAME VARCHAR(20) NOT NULL,
    Age INT NOT NULL,
    Address VARCHAR(25)
);

-- Insert 5 sample records into the Student table
INSERT INTO Student (ID, NAME, Age, Address)
VALUES
    (1, 'John Smith', 21, '123 Main Street'),
    (2, 'Jane Doe', 22, '456 Elm Avenue'),
    (3, 'Bob Johnson', 20, '789 Oak Road'),
    (4, 'Alice Brown', 23, '101 Pine Lane'),
    (5, 'Eva White', 22, '202 Cedar Drive');
